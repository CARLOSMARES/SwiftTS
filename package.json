    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.7.36",
    "jest": "^27.5.1",
    "nodemon": "^3.1.7",
    "typescript": "^4.4.4"
  }
